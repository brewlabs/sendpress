   </php>
   <pearinstaller>
    <min>1.9.4</min>
   </pearinstaller>
   <package>
    <name>PHP_Timer</name>
    <channel>pear.phpunit.de</channel>
    <min>1.0.1</min>
    <max>1.0.99</max>
   </package>
   <extension>
    <name>pcntl</name>
   </extension>
  </required>
