  <name>Sebastian Bergmann</name>
  <user>sb</user>
  <email>sb@sebastian-bergmann.de</email>
  <active>yes</active>
 </lead>
 <date>2013-07-16</date>
 <version>
  <release>1.1.3</release>
  <api>1.1.0</api>
 </version>
