{
    "name": "brewlabs/sendpress",
    "type": "plugin",
    "description": "WordPress Newsletters",
    "keywords": ["wordpress","plugins"],
    "homepage": "http://sendpress.com",
    "license": "GPLv2.0",
    "authors": [
    {
        "name": "SendPress",
        "email": "hello@sendpress.com",
        "role": "Developer"
    }
    ],
    "require": {
        "php": ">=5.2"
        },
        "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                    },
                    "source": {
                        "url": "https://github.com/sebastianbergmann/phpcov.git",
                        "type": "git",
                        "reference": "1.1.0"
                        },
                        "bin": [
                        "phpcov.php"
                        ]
                    }
                }
                ],
                "require-dev": {
                    "satooshi/php-coveralls": "dev-master",
                    "sebastianbergmann/phpcov": "1.1.0",
                    "mikey179/vfsStream": "1.2.0"
                }
            }
        }
    }
